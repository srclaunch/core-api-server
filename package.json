{
  "name": "@srclaunch/core-api-server",
  "description": "AppLab Core API server",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.2",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srclaunch/core-api-server.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/srclaunch/core-api-server/issues"
  },
  "homepage": "https://github.com/srclaunch/core-api-server#readme",
  "scripts": {
    "build:types": "tsc --build --force",
    "build:applab": "yarn applab build",
    "build": "yarn build:applab && yarn build:types",
    "push": "git push --follow-tags origin main"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.2",
    "@srclaunch/dx": "^0.1.2",
    "@srclaunch/types": "^0.1.2",
    "@types/node": "17.0.15",
    "esbuild": "^0.14.19",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@srclaunch/data-client": "^0.1.2",
    "@srclaunch/http-server": "^0.1.2",
    "aws-sdk": "^2.1069.0",
    "change-case": "^4.1.2",
    "config": "^3.3.7",
    "currency-codes": "^2.1.0",
    "is-email": "^1.0.2",
    "keygrip": "^1.1.0",
    "luxon": "^2.3.0",
    "plaid": "^9.10.0",
    "request-ip": "^2.1.3",
    "stripe": "^8.202.0",
    "uuid": "^8.3.2"
  }
}
