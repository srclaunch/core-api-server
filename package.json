{
  "name": "@srclaunch/core-api-server",
  "description": "AppLab Core API server",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.7",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srclaunch/core-api-server.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/srclaunch/core-api-server/issues"
  },
  "homepage": "https://github.com/srclaunch/core-api-server#readme",
  "scripts": {
    "build:types": "tsc --build --force",
    "build:applab": "yarn applab build",
    "build": "yarn clean && yarn build:applab && yarn build:types",
    "clean": "rm -fr ./dist",
    "push": "git push --follow-tags origin main",
    "release": "yarn standard-version && git push --follow-tags origin main",
    "test": "ava",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.18",
    "@srclaunch/dx": "^0.1.15",
    "@srclaunch/types": "^0.1.21",
    "@types/express": "^4.17.13",
    "@types/keygrip": "^1.0.2",
    "@types/luxon": "^2.3.1",
    "@types/node": "17.0.15",
    "@types/query-string": "^6.3.0",
    "ava": "^4.1.0",
    "esbuild": "^0.14.19",
    "sequelize": "^6.17.0",
    "standard-version": "^9.3.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@srclaunch/data-client": "^0.1.4",
    "@srclaunch/exceptions": "^0.1.6",
    "@srclaunch/http-server": "^0.1.5",
    "@srclaunch/logger": "^0.1.2",
    "@srclaunch/node-environment": "^0.1.4",
    "aws-sdk": "^2.1069.0",
    "change-case": "^4.1.2",
    "config": "^3.3.7",
    "currency-codes": "^2.1.0",
    "is-email": "^1.0.2",
    "keygrip": "^1.1.0",
    "luxon": "^2.3.0",
    "plaid": "^9.10.0",
    "request-ip": "^2.1.3",
    "stripe": "^8.202.0",
    "uuid": "^8.3.2"
  }
}
